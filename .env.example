# OpenAI API Key (Required for GPT models)
# Get your key at: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (Required for Claude models)
# Get your key at: https://console.anthropic.com/settings/keys
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Model Selection (Optional)
# Options: fast, balanced, quality, gpt4, gpt4o, claude
VITE_DEFAULT_MODEL=quality

# Unipile Configuration (Required for LinkedIn integration)
# Get your API key at: https://dashboard.unipile.com
VITE_UNIPILE_API_KEY=your_unipile_api_key_here
VITE_UNIPILE_ACCOUNT_ID=your_unipile_account_id_here

# Bright Data Configuration (Required for proxy functionality)
# Get your credentials at: https://brightdata.com
VITE_BRIGHTDATA_CUSTOMER_ID=your_brightdata_customer_id_here
VITE_BRIGHTDATA_PASSWORD=your_brightdata_password_here

# n8n Configuration (Required for workflow automation)
VITE_N8N_URL=https://workflows.innovareai.com

# Feature Flags
VITE_ENABLE_VOICE=true
VITE_ENABLE_LINKEDIN=true
VITE_ENABLE_EMAIL=true